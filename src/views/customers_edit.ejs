<%- include('partials/navbar') %>

<div class="container mt-5">
    <div class="row"> 
        <div class="col-md-5">            
            <div class="card">
                <div class="card-body">                        
                       <form action="/update/<%= data.id %>" method="post">
                        <div class="from-group">
                            <input type="text" name="name" value="<%= data.name %>" class="from-control">
                        </div>
                        <div class="from-group">
                            <input type="text" name="address"  value="<%= data.address %>" class="from-control">
                        </div>
                        <div class="from-group">
                            <input type="text" name="phone"  value="<%= data.phone %>" class="from-control">
                        </div>
                        <button type="submit" class="btn btn-primary">
                            Actualizar Customer
                        </button> 
                       </form>
                </div>
            </div>                                
        </div>
    </div>
</div>





<%- include('partials/footer') %>